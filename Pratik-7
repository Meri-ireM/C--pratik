#include<stdio.h>
int main() {
	int sayi1,sayi2;
	printf("Iki tane sayi giriniz lutfen:\n");
	scanf("%d\n %d",&sayi1,&sayi2);
	printf("Girdiginiz sayilarin toplami: %d ", sayi1+sayi2);
	
	
	return 0;
}

